{
  "name": "seaice-blog",
  "version": "1.0.0",
  "description": "SeaiCe 's Blog",
  "main": "index.js",
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server --config ./config/webpack.client.config.js",
    "serve": "webpack --config ./config/webpack.server.config.js && node build/server.bundle.js",
    "build:client": "rimraf build/client/* && cross-env NODE_ENV=production webpack --config ./config/webpack.client.prod.config.js",
    "build:server": "rimraf build/server/* && webpack --config ./config/webpack.server.config.js",
    "analyse": "npm run build -- --profile --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/sea-ice/seaice-blog.git"
  },
  "keywords": [
